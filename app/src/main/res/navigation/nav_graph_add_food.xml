<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_add_food"
    app:startDestination="@id/foodListFragment">

    <fragment
        android:id="@+id/foodListFragment"
        android:name="Screens.Registration.ThirdAdd.Food.FoodListFragment"
        android:label="fragment_food_list"
        tools:layout="@layout/fragment_food_list" >
        <action
            android:id="@+id/action_foodListFragment_to_newFoodAddFragment"
            app:destination="@id/newFoodAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/newFoodAddFragment"
        android:name="Screens.Registration.ThirdAdd.Food.NewFoodAddFragment"
        android:label="fragment_new_food_add"
        tools:layout="@layout/fragment_new_food_add" >
        <action
            android:id="@+id/action_newFoodAddFragment_to_foodListFragment"
            app:destination="@id/foodListFragment" />
    </fragment>
</navigation>